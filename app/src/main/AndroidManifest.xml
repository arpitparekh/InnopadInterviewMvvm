        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.InnopadInterviewMvvm"
        tools:targetApi="31">
        <activity
            android:name=".task_two.ShowAddressActivity"
            android:exported="true" >

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
        <activity
            android:name=".task_two.AddAddressActivity"
            android:exported="true" >



        </activity>
        <activity
            android:name=".task_one.ShowUserDataActivity"
            android:exported="true" />
        <activity
            android:name=".task_one.LoginScreenActivity"
            android:exported="true">

        </activity>
        <activity
            android:name=".task_one.AddUserDataActivity"
            android:exported="false" />
    </application>

</manifest>